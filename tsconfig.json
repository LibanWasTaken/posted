{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "*": ["*", "*/index.tsx", "*/index.ts"]
    },
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "outDir": "./dist"
  },
  "exclude": [
    "./dist/**/*" // This is what fixed it!
  ]
}
